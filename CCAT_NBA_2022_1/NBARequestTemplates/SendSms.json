{
	"CustomerMarketingProduct": [
		{
			"id": [
				{
					"value": "$PromoID Value$"
				}
			],
			"type": "NBAOffers",
			"category": [
				{
					"value": "$category$",
					"listHierarchyId": "TriggerID"
				}
			],
			"parts": {
				"channel": [
					{
						"id": [
							{
								"value": "$channelId$"
							}
						]
					}
				],
				"customerAccount": [
					{
						"id": [
							{
								"value": "$customerAccountId$"
							}
						]
					}
				],
				"specification": {
					"characteristicsValue": [
						{
							"characteristicName": "shortCode",
							"value": "$shortCode$"
						},
						{
							"characteristicName": "agentId",
							"value": "$agentId$"
						},
						{
							"characteristicName": "storeId",
							"value": "$storeId$"
						},
						{
							"characteristicName": "giftSeqID",
							"value": "$giftSeqID$"
						}
					]
				}
			}
		}
	]
}